{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "Example extension that uses the chrome.tabs API.",
    "permissions": ["tabs"],
    "action": {
      "default_icon": "icon.png",
      "default_title": "Get Current Tab's URL",
      "default_popup": "popup.html"
    }
  }